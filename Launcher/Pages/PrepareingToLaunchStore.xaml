<Page x:Class="Launcher.Pages.PrepareingToLaunchStore"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Launcher.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="869" d:DesignWidth="1538"
      Title="PrepareingToLaunchStore">

    <Grid>
        <Path
            Data="{StaticResource RustyLogo}"
            Width="75"
            Height="75"
            Margin="0,0,0,0"
            VerticalAlignment="Center"
            />
        <ProgressBar
            x:Name="Progress"
            Foreground="{StaticResource ProgressBar.Progress}"
            Height="150"
            Width="150"
            Margin="0,0,0,0"
            Visibility="Visible"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            IsIndeterminate="True"
            Style="{StaticResource CircularProgressBar}"/>
    </Grid>
</Page>
